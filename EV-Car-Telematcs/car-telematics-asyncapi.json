{
  "asyncapi": "2.6.0",
  "info": {
    "title": "Car Telematics API",
    "version": "1.0.0",
    "description": "Car Telematics API for EV connected car telematics system.\nHandles various events including location updates, speed monitoring, battery status, and vehicle diagnostics.\n"
  },
  "channels": {
    "vehicle/location/update/{vin}/{region}/{timestamp}": {
      "subscribe": {
        "summary": "Real-time vehicle location updates",
        "message": {
          "name": "locationUpdate",
          "payload": {
            "type": "object",
            "properties": {
              "latitude": {
                "type": "number",
                "format": "float",
                "description": "Vehicle's current latitude"
              },
              "longitude": {
                "type": "number",
                "format": "float",
                "description": "Vehicle's current longitude"
              },
              "altitude": {
                "type": "number",
                "format": "float",
                "description": "Vehicle's current altitude in meters"
              },
              "speed": {
                "type": "number",
                "description": "Current speed in km/h"
              },
              "heading": {
                "type": "number",
                "description": "Vehicle heading in degrees"
              },
              "accuracy": {
                "type": "number",
                "description": "GPS accuracy in meters"
              }
            }
          },
          "headers": {
            "type": "object",
            "properties": {
              "correlation-id": {
                "type": "string",
                "format": "uuid"
              }
            }
          }
        }
      },
      "parameters": {
        "vin": {
          "description": "Vehicle Identification Number",
          "schema": {
            "type": "string"
          }
        },
        "region": {
          "description": "Geographic region code",
          "schema": {
            "type": "string"
          }
        },
        "timestamp": {
          "description": "Event timestamp",
          "schema": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "vehicle/speed/exceed/{vin}/{speed_limit}/{current_speed}/{location_id}/{timestamp}": {
      "subscribe": {
        "summary": "Speed limit violation events",
        "message": {
          "name": "speedExceed",
          "payload": {
            "type": "object",
            "properties": {
              "duration": {
                "type": "number",
                "description": "Duration of speed limit violation in seconds"
              },
              "road_type": {
                "type": "string",
                "enum": [
                  "highway",
                  "urban",
                  "residential",
                  "school_zone"
                ]
              },
              "weather_conditions": {
                "type": "string"
              },
              "traffic_conditions": {
                "type": "string"
              }
            }
          },
          "headers": {
            "type": "object",
            "properties": {
              "severity": {
                "type": "string",
                "enum": [
                  "warning",
                  "violation"
                ]
              }
            }
          }
        }
      },
      "parameters": {
        "vin": {
          "description": "Vehicle Identification Number",
          "schema": {
            "type": "string"
          }
        },
        "speed_limit": {
          "description": "Posted speed limit",
          "schema": {
            "type": "number"
          }
        },
        "current_speed": {
          "description": "Current vehicle speed",
          "schema": {
            "type": "number"
          }
        },
        "location_id": {
          "description": "Location identifier where violation occurred",
          "schema": {
            "type": "string"
          }
        },
        "timestamp": {
          "description": "Event timestamp",
          "schema": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "vehicle/battery/status/{vin}/{charging_state}/{location_id}/{timestamp}": {
      "subscribe": {
        "summary": "Battery status updates",
        "message": {
          "name": "batteryStatus",
          "payload": {
            "type": "object",
            "properties": {
              "charge_level": {
                "type": "number",
                "description": "Current battery charge percentage"
              },
              "range_estimate": {
                "type": "number",
                "description": "Estimated range in kilometers"
              },
              "voltage": {
                "type": "number",
                "description": "Battery voltage"
              },
              "temperature": {
                "type": "number",
                "description": "Battery temperature in Celsius"
              },
              "charging_rate": {
                "type": "number",
                "description": "Current charging rate in kW"
              },
              "health_status": {
                "type": "string",
                "enum": [
                  "optimal",
                  "degraded",
                  "service_required"
                ]
              }
            }
          },
          "headers": {
            "type": "object",
            "properties": {
              "priority": {
                "type": "string",
                "enum": [
                  "normal",
                  "high"
                ]
              }
            }
          }
        }
      },
      "parameters": {
        "vin": {
          "description": "Vehicle Identification Number",
          "schema": {
            "type": "string"
          }
        },
        "charging_state": {
          "description": "Current charging state",
          "schema": {
            "type": "string",
            "enum": [
              "charging",
              "discharging",
              "idle"
            ]
          }
        },
        "location_id": {
          "description": "Current location identifier",
          "schema": {
            "type": "string"
          }
        },
        "timestamp": {
          "description": "Event timestamp",
          "schema": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "vehicle/diagnostics/alert/{vin}/{system}/{severity}/{component_id}/{timestamp}": {
      "subscribe": {
        "summary": "Vehicle diagnostic alerts",
        "message": {
          "name": "diagnosticAlert",
          "payload": {
            "type": "object",
            "properties": {
              "error_code": {
                "type": "string"
              },
              "description": {
                "type": "string"
              },
              "recommended_action": {
                "type": "string"
              },
              "maintenance_required": {
                "type": "boolean"
              },
              "diagnostic_data": {
                "type": "object",
                "additionalProperties": true
              }
            }
          },
          "headers": {
            "type": "object",
            "properties": {
              "alert_id": {
                "type": "string",
                "format": "uuid"
              }
            }
          }
        }
      },
      "parameters": {
        "vin": {
          "description": "Vehicle Identification Number",
          "schema": {
            "type": "string"
          }
        },
        "system": {
          "description": "Vehicle system reporting the alert",
          "schema": {
            "type": "string",
            "enum": [
              "powertrain",
              "battery",
              "brake",
              "suspension",
              "hvac",
              "infotainment"
            ]
          }
        },
        "severity": {
          "description": "Alert severity level",
          "schema": {
            "type": "string",
            "enum": [
              "info",
              "warning",
              "critical"
            ]
          }
        },
        "component_id": {
          "description": "Specific component identifier",
          "schema": {
            "type": "string"
          }
        },
        "timestamp": {
          "description": "Event timestamp",
          "schema": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    }
  },
  "components": {
    "messages": {
      "locationUpdate": {
        "name": "locationUpdate",
        "title": "Vehicle Location Update",
        "summary": "Real-time update of vehicle location and movement data",
        "contentType": "application/json",
        "headers": {
          "type": "object",
          "properties": {
            "correlation-id": {
              "type": "string",
              "format": "uuid"
            }
          }
        }
      },
      "speedExceed": {
        "name": "speedExceed",
        "title": "Speed Limit Violation",
        "summary": "Event indicating vehicle has exceeded local speed limit",
        "contentType": "application/json",
        "headers": {
          "type": "object",
          "properties": {
            "severity": {
              "type": "string",
              "enum": [
                "warning",
                "violation"
              ]
            }
          }
        }
      },
      "batteryStatus": {
        "name": "batteryStatus",
        "title": "Battery Status Update",
        "summary": "Current battery status and health information",
        "contentType": "application/json",
        "headers": {
          "type": "object",
          "properties": {
            "priority": {
              "type": "string",
              "enum": [
                "normal",
                "high"
              ]
            }
          }
        }
      },
      "diagnosticAlert": {
        "name": "diagnosticAlert",
        "title": "Diagnostic Alert",
        "summary": "Vehicle diagnostic alert for various systems",
        "contentType": "application/json",
        "headers": {
          "type": "object",
          "properties": {
            "alert_id": {
              "type": "string",
              "format": "uuid"
            }
          }
        }
      }
    }
  }
}