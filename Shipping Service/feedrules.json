[
  {
    "topic": "acmeretail/onlineservices/order/shipped/v1/{regionId}/{orderId}",
    "eventName": "Order Shipped",
    "eventVersion": "1.0.1",
    "messageName": "Order_Shipped",
    "topicParameters": {
      "regionId": {
        "schema": {
          "type": "string"
        },
        "rule": {
          "name": "regionId",
          "type": "string",
          "group": "StringRules",
          "rule": "enum",
          "enum": [
            "AMER",
            "UKI",
            "APAC",
            "MENA"
          ]
        }
      },
      "orderId": {
        "schema": {
          "type": "string"
        },
        "rule": {
          "name": "orderId",
          "type": "string",
          "group": "StringRules",
          "rule": "alpha",
          "casing": "mixed",
          "minLength": 10,
          "maxLength": 10
        }
      }
    },
    "payload": {
      "order_date": {
        "format": "date-time",
        "description": "Date and time when the order was placed",
        "type": "string",
        "rule": {
          "name": "order_date",
          "type": "string",
          "group": "DateRules",
          "rule": "recent",
          "days": 10
        }
      },
      "total_price": {
        "description": "Total price of the order",
        "type": "number",
        "minimum": 0,
        "rule": {
          "name": "total_price",
          "type": "number",
          "group": "FinanceRules",
          "rule": "amount"
        }
      },
      "order_id": {
        "description": "Unique identifier for the order",
        "type": "string",
        "rule": {
          "name": "order_id",
          "type": "string",
          "group": "StringRules",
          "rule": "numeric",
          "minLength": 5,
          "maxLength": 5,
          "leadingZeros": true
        }
      },
      "items": {
        "description": "Items included in the order",
        "type": "array",
        "subType": "object",
        "name": "items",
        "properties": {
          "quantity": {
            "description": "Quantity of the item ordered",
            "type": "integer",
            "minimum": 1,
            "rule": {
              "name": "quantity",
              "type": "integer",
              "group": "NumberRules",
              "rule": "int",
              "minimum": 1,
              "maximum": 20
            }
          },
          "item_id": {
            "description": "Unique identifier for the item",
            "type": "string",
            "rule": {
              "name": "item_id",
              "type": "string",
              "group": "StringRules",
              "rule": "numeric",
              "minLength": 3,
              "maxLength": 3,
              "leadingZeros": true
            }
          },
          "price": {
            "description": "Price per unit of the item",
            "type": "number",
            "minimum": 0,
            "rule": {
              "name": "price",
              "type": "number",
              "group": "CommerceRules",
              "rule": "price",
              "minimum": 1,
              "maximum": 999
            }
          }
        },
        "rule": {
          "name": "items",
          "type": "object"
        }
      },
      "customer": {
        "type": "object",
        "properties": {
          "name": {
            "description": "Name of the customer",
            "type": "string",
            "rule": {
              "name": "name",
              "type": "string",
              "group": "PersonRules",
              "rule": "fullName"
            }
          },
          "id": {
            "description": "Unique identifier for the customer",
            "type": "string",
            "rule": {
              "name": "id",
              "type": "string",
              "group": "StringRules",
              "rule": "numeric",
              "minLength": 5,
              "maxLength": 5,
              "leadingZeros": true
            }
          },
          "email": {
            "format": "email",
            "description": "Email address of the customer",
            "type": "string",
            "rule": {
              "name": "email",
              "type": "string",
              "group": "StringRules",
              "rule": "fromRegExp",
              "pattern": "[a-zA-Z]{5,19}@[a-zA-Z0-9]{5,7}.com"
            }
          }
        },
        "required": [
          "id",
          "name",
          "email"
        ],
        "rule": {
          "name": "customer",
          "type": "object"
        }
      },
      "status": {
        "description": "Status of the order",
        "type": "string",
        "enum": [
          "pending",
          "processing",
          "shipped",
          "delivered",
          "cancelled"
        ],
        "rule": {
          "name": "status",
          "type": "string",
          "group": "StringRules",
          "rule": "enum",
          "enum": [
            "pending",
            "processing",
            "shipped",
            "delivered",
            "cancelled"
          ]
        }
      }
    },
    "publishSettings": {
      "count": 20,
      "interval": 1,
      "delay": 0
    },
    "mappings": [
      {
        "type": "Topic Parameter",
        "source": {
          "type": "Payload Parameter",
          "name": "order_id",
          "fieldName": "order_id",
          "fieldType": "string"
        },
        "target": {
          "type": "Topic Parameter",
          "name": "orderId",
          "fieldName": "orderId",
          "fieldType": "string"
        }
      }
    ]
  }
]